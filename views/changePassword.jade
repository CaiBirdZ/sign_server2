doctype html
html
    head
        title= title
        link(rel='stylesheet', href='/stylesheets/style.css')
        script(src='/javascripts/jquery-3.1.1.js')
        script(src='/javascripts/changePassword.js')
    body
        block content
            div.Rect
                div
                    span.tipText 账号
                    input.usedInput#usernum(type="text",style='margin-left:12.5%')
                div
                    span.tipText 旧密码
                    input.usedInput#oldPassword(type='password',style='margin:3% 0 0 10%')
                div
                    span.tipText  新密码
                    input.usedInput#newPassword(type='password',style='margin:3% 0 0 10%')
                div
                    span.tipText  新密码确认
                    input.usedInput#confirmPassword(type='password',style='margin:3% 0 0 5%')
                div
                    span.tipText(style='color:#c73d4f')  公司密钥
                    input.usedInput#cpKey(type='text',style='margin-left:7%;border: 3px solid #c73d4f')
                div
                    button.usedButton(onclick='submit()') 修改
